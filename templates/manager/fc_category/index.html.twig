

{% block title %}FcCategory index{% endblock %}

{% block body %}
 <h1>Create new FcCategory</h1>

    {{ include('manager/fc_category/_form.html.twig') }}

    <h1>FcCategory index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Topic</th>
                <th>CreatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for fc_category in fc_categories %}
            <tr>
                <td>{{ fc_category.id }}</td>
                <td>{{ fc_category.topic }}</td>
                <td>{{ fc_category.createdAt ? fc_category.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('fc_category_edit', {'id': fc_category.id}) }}">edit</a>
                    {{ include('manager/fc_category/_delete_form.html.twig') }}
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
