{% extends 'base.html.twig' %}

{% block title %}FcCategory{% endblock %}

{% block body %}
    <h1>FcCategory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fc_category.id }}</td>
            </tr>
            <tr>
                <th>Topic</th>
                <td>{{ fc_category.topic }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ fc_category.createdAt ? fc_category.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('fc_category_index') }}">back to list</a>

    <a href="{{ path('fc_category_edit', {'id': fc_category.id}) }}">edit</a>

    {{ include('fc_category/_delete_form.html.twig') }}
{% endblock %}
